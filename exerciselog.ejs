<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tracker</title>
    <link rel="stylesheet" href="/styles/exerciselog.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>

<body id="body" onload="updateDateTime">
    <%- include('partials/navbar') %>
    <span id="datetime"></span>
    <table class="WorkoutLogger">
        <thead>
            <tr>
                <td scope="col" class="topRow">Activity Name</th>
                <td scope="col" class="topRow">Duration</th>
                <td scope="col" class="topRow">Calories burned</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td id="workoutName" class="trow">Treadmill</td>
                <td id="duration" class="trow"> 30 min</td>
                <td id="dataCal" class="trow">100</td>
                <!-- <td class="deleterow"><a href="#">Delete</a></td> -->
            </tr>
            <tr>
                <td id="workoutName" class="trow">Stair Master</td>
                <td id="duration" class="trow"> 30 min</td>
                <td id="dataCal" class="trow">100</td>
                <!-- <td class="deleterow"><a href="#">Delete</a></td> -->
            </tr>
            <tr>
                <td id="workoutName" class="trow">Eliptic</td>
                <td id="duration" class="trow"> 30 min</td>
                <td id="dataCal" class="trow">100</td>
                <!-- <td class="deleterow"><a href="#">Delete</a></td> -->
            </tr>
            <tr>
                <td id="workoutName" class="trow">Cycling</td>
                <td id="duration" class="trow"> 30 min</td>
                <td id="dataCal" class="trow">100</td>
                <!-- <td class="deleterow"><a href="#">Delete</a></td> -->
            </tr>
            <tr class="bottom">
                <td class="addExercise"><a href="add_to_log">Add Exercise</a></td>
                <!-- <td></td>
                <td></td> -->
                <!-- <td></td> -->
            </tr>
        </tbody>
        <tfoot>
            <tr>
                <td>
                    <span class="Daily"></span>
                    <span class="Goal"></span>
                </td>
            </tr>
        </tfoot>
    </table>




    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
    <script src="/scripts/exerciselog.js"></script>
</body>

</html>
